APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_NETWORK_NAME=app_network
APP_BASE_PATH=/usr/local/www/app
EXPOSE_HTTP_PORT=8008

EXPOSE_RABBITMQ_PORT=5674
EXPOSE_RABBITMQ_WEB_PORT=15674

LOG_CHANNEL=stack

DB_CONNECTION=mysql
DB_HOST=127.0.0.1
